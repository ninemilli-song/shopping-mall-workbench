{
    "extends": ["eslint-config-airbnb"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/no-multi-comp": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-indent": [2, 4],
      "react/jsx-indent-props": [2, 4],
      "react/prop-types": 0,
      "react/no-danger": 0,
      "import/default": 0,
      "import/no-duplicates": 0,
      "import/named": 0,
      "import/namespace": 0,
      "import/no-unresolved": 0,
      "import/no-named-as-default": 2,
      "import/extensions": 0,
      "comma-dangle": 0,
      "indent": [2, 4, {"SwitchCase": 1}],
      "no-console": 0,
      "no-alert": 0,
      "arrow-body-style": 0,
      "global-require": 0,
      "prefer-spread": 0,
      "no-param-reassign": 0,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "quote-props": 0,
      "import/no-dynamic-require": 0,
      "jsx-a11y/html-has-lang": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/click-events-have-key-events": 0
    },
    "plugins": [
      "react",
      "import"
    ],
    "settings": {
      "import/parser": "babel-eslint",
      "import/resolve": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    },
    "globals": {
      "__DEV__": true,
      "__dirname": true
    }
  }
  